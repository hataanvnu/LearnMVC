@model QuizFinishedVM

<div class="container-fluid">
    <div id="bigwhite" class="col-md-8" style="margin:3%;float:right">

        Good job! You just finished this category! 
        <img src="http://www.animatedimages.org/data/media/1103/animated-congratulation-image-0092.gif" border="0" alt="animated-congratulation-image-0092" />

        <button class="btn btn-default" onclick="Next(@Model.CategoryId)">Next Category!</button> 

    </div>
    <!--HERE IS SIDEBOX-->
    <div class="col-md-3" id="sidebox">
        <div class="row" id="logobox" style="padding-top:10px">
            <img src="~/Images/LearnMVClogo.png" width="170" style="float:left" />
        </div>

        <div>
            @Html.Partial("_Sidebar", Model.SidebarArray)
        </div>
    </div>
</div> <!--END ROW-->
